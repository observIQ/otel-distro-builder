<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>__DISTRIBUTION__</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/__DISTRIBUTION__/supervisor</string>
        <string>--config</string>
        <string>/opt/__DISTRIBUTION__/supervisor-config.yaml</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>WorkingDirectory</key>
    <string>/opt/__DISTRIBUTION__</string>
    <key>ExitTimeOut</key>
    <integer>20</integer>
    <key>StandardErrorPath</key>
    <string>/var/log/__DISTRIBUTION__.err</string>
</dict>
</plist>
