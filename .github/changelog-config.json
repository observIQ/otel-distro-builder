{
    "categories": [
        {
            "title": "## ğŸš€ Features",
            "labels": ["feature", "enhancement"]
        },
        {
            "title": "## ğŸ› Fixes",
            "labels": ["fix", "bug"]
        },
        {
            "title": "## ğŸ“ Documentation",
            "labels": ["documentation"]
        },
        {
            "title": "## ğŸ”§ Maintenance",
            "labels": ["chore", "dependencies"]
        }
    ],
    "template": "${{CHANGELOG}}\n\n## ğŸ”— Links\n- [Full Changelog](${{RELEASE_DIFF}})\n\n## ğŸ‘¥ Contributors\n${{CONTRIBUTORS}}",
    "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
    "empty_template": "- No changes in this release",
    "label_extractor": [
        {
            "pattern": "(.) (.+)",
            "target": "$1",
            "flags": "gu"
        }
    ],
    "transformers": [
        {
            "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
            "target": "* $4\n  $6"
        }
    ],
    "max_tags_to_fetch": 200,
    "max_pull_requests": 200,
    "max_back_track_time_days": 365,
    "exclude_merge_branches": [
        "Owner/qa"
    ],
    "tag_resolver": {
        "method": "semver"
    },
    "base_branches": [
        "main"
    ]
} 